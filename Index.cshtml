@model List<crud.Models.employee>
@{
    ViewData["Title"] = "Index";
}
<a href="@Url.Action("insert","Home")" class="btn btn-primary">Insert</a>
<table class="table table-bordered table-hover">
    <thead>
        <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Edit</th>
        <th>Delete</th>

        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Mobile</td>
            <td><a href="@Url.Action("update","Home",new{item.ID})">Update</a></td>
           <td><a href="@Url.Action("Delete","Home",new{item.ID})">Delete</a></td>
        </tr>
        }
    </tbody>
</table>
<h1>Index</h1>

